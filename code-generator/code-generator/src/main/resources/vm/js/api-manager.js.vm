import request from '@/utils/request'

// 查询${functionName}列表
export function list${BusinessName}(params) {
  return getRequest('/${moduleName}/${businessName}', params);
}

// 查询${functionName}详细
export function get${BusinessName}(${pkColumn.javaField}) {
  return getRequest('/${moduleName}/${businessName}' + ${pkColumn.javaField});
}

// 新增${functionName}
export function add${BusinessName}(data) {
  return postRequest('/${moduleName}/${businessName}', data);
}

// 修改${functionName}
export function update${BusinessName}(data) {
  return putRequest('/${moduleName}/${businessName}', data);
}

// 删除${functionName}
export function del${BusinessName}(${pkColumn.javaField}) {
  return deleteRequest('/${moduleName}/${businessName}/' + ${pkColumn.javaField});
}